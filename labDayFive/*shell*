a#include <iostream>
#include <fstream>
using namespace std;
int main()
{
  ifstream instream;
  char letter;
  int i,j;
  cout << "CHARACTER";
  cout.width(26);
  cout << "OCCURRENCES" << endl;
  instream.open("ex5.cpp");
  for (letter = 'a', i = 0; letter <= 'z'; letter++)
    {
      cout << letter;
      while(!instream.eof())
	{  
	  cout << (char)instream.get();    
	  //  if(letter == (char)instream.get())
	  //  i++;
	}
      cout.width(25);
      cout << i << endl;
    }

}
